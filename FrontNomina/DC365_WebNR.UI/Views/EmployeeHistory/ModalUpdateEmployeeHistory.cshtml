

<form asp-action="HireEmployee" class="TinyModal-hire-employee" id="Update-history-employee">
    <h3 class="TituloEstandar-modal-contratar">Actualizar fecha</h3>
    <div class="cont-input-newform-col1">

        <div class="contlotesmploye">
            <label class="control-label labelModEmployeComen">Id de historial</label>
            <div class="">
                <input name="EmployeeHistoryId" class="form-control EmployeeHistoryId-update" readonly="readonly" />
            </div>
        </div>
        <div class="contlotesmploye">
            <label class="control-label labelModEmployeComen">Fecha del registro</label>
            <div class="">
                <div class="app-input-date">
                    <input name="RegisterDate" class="form-control" type="date" />
