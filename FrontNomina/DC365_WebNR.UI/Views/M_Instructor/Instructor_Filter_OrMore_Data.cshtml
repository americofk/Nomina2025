@model IEnumerable<DC365_WebNR.CORE.Domain.Models.Instructor>
@{int indexCheck = 1 + ((int)ViewBag.CountPageNumber * 20); }
@foreach (var item in Model)
{

    string idCheck = $"check-table-{indexCheck++}";
    <tr class="row-app">

        <td class="cell-app check-cell-app">
            <input id="@idCheck" type="checkbox" class="check-table-app selectInstructor">
            <label for="@idCheck" class="label-cell"></label>
        </td>
        <td class="cell-app cell-principal InstructorIdtblIns" data-title="Código">
            @Html.DisplayFor(modelItem => item.InstructorId)
        </td>
        <td class="cell-app NametblIns" data-title="Nombre">
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td class="cell-app PhonetblIns" data-title="Teléfono)">
            @Html.DisplayFor(modelItem => item.Phone)
        </td>

        <td class="cell-app MailtblIns" data-title="Email)">
            @Html.DisplayFor(modelItem => item.Mail)
        </td>

        <td class="cell-app CompanytblIns" data-title="Empresa)">
            @Html.DisplayFor(modelItem => item.Company)
        </td>

        <td class="cell-app CommenttblIns" data-title="Comentario)">
            @Html.DisplayFor(modelItem => item.Comment)
        </td>


    </tr>
}