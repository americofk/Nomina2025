@model DC365_WebNR.CORE.Domain.Models.ProjCategory
    <!--Opciones de vista parcial -->
    <div class="opcioneslistpageForm collapse">
        <div class="menuoptions-flex">
            <ul class="menuoptions">
                <li>
                    <a class="btn2 btn-app">
                        <i class="fa fa-save"></i>
                        <input type="submit" value="Guardar" form="createAndEditProjectCategory" class="btn2" />
                    </a>
                </li>
            </ul>

            <ul class="menuoptions">
                <li class="OpCloseform">
                    <a class="btn2 btn-app ">
                        <i class="fa fa-close i-close"></i>
                    </a>
                </li>
            </ul>
        </div>
    </div>


    <!-- nuevo departamento -->
    <div class="collapse contNewEntyti">
        <input type="text" class="form-control codigoDeparment" style="display:none">
        <div class="col-md-12 col-sm-12 col-xs-12 margenFormmularios ContenedorFormularios2">
            <div class="x_panel">
                <div class="x_content">
                    <h3 class="TituloFormularios">Nueva categoría de proyecto</h3>
                    <form class="contenidoNuevoGasto2" role="tabpanel" data-example-id="togglable-tabs" id="createAndEditProjectCategory" asp-action="save">

                        <div class="contInterNuevoConteGastos">

                            <div class="contlotes Showid collapse">
                                <label class="control-label ancholabel">Código de categoría</label>
                                <div class="anchoimput">
                                    <input asp-for="ProjCategoryId" class="form-control" readonly="readonly" />
                                    <span asp-validation-for="ProjCategoryId" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="contlotes">
                                <label class="control-label ancholabel">Nombre de categoría</label>
                                <div class="anchoimput">
                                    <input asp-for="CategoryName" class="form-control" />
                                    <span asp-validation-for="CategoryName" class="text-danger"></span>
                                </div>

                            </div>




                        </div>
                        <div class="contInterNuevoConteGastos">

                            <div class="contlotes">
                                <label class="control-label ancholabel">Nº de cuenta contable</label>
                                <div class="anchoimput">
                                    <input asp-for="LedgerAccount" class="form-control" />
                                    <span asp-validation-for="LedgerAccount" class="text-danger"></span>
                                </div>

                            </div>

                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
